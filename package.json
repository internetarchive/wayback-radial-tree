  "description": "Radial Tree Library for Wayback Machine",
  "main": "build/radial-tree.cjs.js",
  "module": "build/radial-tree.esm.js",
  "browser": "build/radial-tree.umd.js",
  "browserslist": "> 0.25%, not dead",
