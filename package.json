    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "html-webpack-plugin": "^5.5.0",
    "mocha": "^10.7.0",
    "mochapack": "^2.1.4",
    "postcss": "^8.4.40",
    "rollup": "^3.29",
