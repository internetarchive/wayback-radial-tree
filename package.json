  "keywords": [
    "Wayback Machine",
    "Radial Tree Library"
  ],
  "dependencies": {
    "d3": "^7.9.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.25",
    "@babel/eslint-parser": "^7.25",
    "@babel/plugin-external-helpers": "^7.24",
    "@babel/preset-env": "^7.25",
